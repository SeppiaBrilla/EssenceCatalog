         -
         min([[east_Function1D[c1], east_Function1D[c2]; int(1..2)][q14]
                  | q14 : int(1..2)])
         >= 0
         \/
         max([[north_Function1D[c1], north_Function1D[c2]; int(1..2)][q17]
                  | q17 : int(1..2)])
         -
         min([[south_Function1D[c1], south_Function1D[c2]; int(1..2)][q20]
                  | q20 : int(1..2)])
