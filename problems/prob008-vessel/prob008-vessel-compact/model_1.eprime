                        separation_FunctionAsRelation_RelationAsSet_Explicit_1_Occurrence[q6, q21]])
             /\
             and([separation_FunctionAsRelation_RelationAsSet_Explicit_1_Occurrence[q6,
                                                                                    [class_Function1D[c1],
                                                                                     class_Function1D[c2]; int(1..2)][q27]]
                      | q27 : int(1..2)])
             /\
             separation_FunctionAsRelation_RelationAsSet_Explicit_2[q6] <=
             max([[west_Function1D[c1], west_Function1D[c2]; int(1..2)][q30]
                      | q30 : int(1..2)])
             -
             min([[east_Function1D[c1], east_Function1D[c2]; int(1..2)][q33]
                      | q33 : int(1..2)])
                 | q6 : int(1..fin4)])
         \/
         or([and([or([[class_Function1D[c1], class_Function1D[c2]; int(1..2)][q37] = q34
                          | q37 : int(1..2)])
                      | q34 : int(1..n_classes),
                        separation_FunctionAsRelation_RelationAsSet_Explicit_1_Occurrence[q8, q34]])
             /\
             and([separation_FunctionAsRelation_RelationAsSet_Explicit_1_Occurrence[q8,
                                                                                    [class_Function1D[c1],
                                                                                     class_Function1D[c2]; int(1..2)][q40]]
                      | q40 : int(1..2)])
             /\
             separation_FunctionAsRelation_RelationAsSet_Explicit_2[q8] <=
             max([[north_Function1D[c1], north_Function1D[c2]; int(1..2)][q43]
                      | q43 : int(1..2)])
             -
             min([[south_Function1D[c1], south_Function1D[c2]; int(1..2)][q46]
                      | q46 : int(1..2)])
                 | q8 : int(1..fin4)])
