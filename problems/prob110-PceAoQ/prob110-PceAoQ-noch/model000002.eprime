             | q2 : int(1..(1 + (n - 1)) * (1 + (n - 1)) - 1)]),
    and([white_ExplicitVarSizeWithFlags_Flags[q2] = false -> white_ExplicitVarSizeWithFlags_Values_1[q2] = 1 /\ white_ExplicitVarSizeWithFlags_Values_2[q2] = 1
             | q2 : int(1..(1 + (n - 1)) * (1 + (n - 1)))]),
    and([white_ExplicitVarSizeWithFlags_Flags[q2 + 1] -> white_ExplicitVarSizeWithFlags_Flags[q2] | q2 : int(1..(1 + (n - 1)) * (1 + (n - 1)) - 1)])

