language ESSENCE' 1.0

given k: int(1..)
given b: int(1..)
given t: int(1..)
given g: int(2..)
where k >= t
where b >= g ** t
find CoverTest_ExplicitWithRepetitionR9_Flag: int(0..b * b)
find CoverTest_ExplicitWithRepetitionR9_Values_Function1D:
        matrix indexed by [int(1..b * b), int(1..k)] of int(1..g)
such that
    and([or([q12 <= CoverTest_ExplicitWithRepetitionR9_Flag /\
             and([CoverTest_ExplicitWithRepetitionR9_Values_Function1D[q12, q14] =
                  testcase_Function1DPartial_Values[q14]
                      | q14 : int(1..k), testcase_Function1DPartial_Flags[q14]])
                 | q12 : int(1..b * b)])
             | testcase_Function1DPartial_Flags : matrix indexed by [int(1..k)] of bool,
               testcase_Function1DPartial_Values : matrix indexed by [int(1..k)] of int(1..g),
               and([testcase_Function1DPartial_Flags[q1] = false ->
                    testcase_Function1DPartial_Values[q1] = 1
                        | q1 : int(1..k)]),
               sum([1 | q11 : int(1..k), testcase_Function1DPartial_Flags[q11]]) = t]),
    and([q4 + 1 <= CoverTest_ExplicitWithRepetitionR9_Flag ->
         CoverTest_ExplicitWithRepetitionR9_Values_Function1D[q4, ..] <=lex
         CoverTest_ExplicitWithRepetitionR9_Values_Function1D[q4 + 1, ..]
             | q4 : int(1..b * b - 1)]),
    and([q5 > CoverTest_ExplicitWithRepetitionR9_Flag ->
         and([CoverTest_ExplicitWithRepetitionR9_Values_Function1D[q5, q15] = 1
                  | q15 : int(1..k)])
             | q5 : int(1..b * b)]),
    b = CoverTest_ExplicitWithRepetitionR9_Flag

