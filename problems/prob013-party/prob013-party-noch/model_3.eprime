    and([and([hosts_ExplicitVarSizeWithFlags_Flags[q5] ->
              sched_Explicit_Function1D[q4, hosts_ExplicitVarSizeWithFlags_Values[q5]] = hosts_ExplicitVarSizeWithFlags_Values[q5]
                  | q5 : int(1..1 + (n_boats - 1))])
             | q4 : int(1..n_periods)]),
    and([and([hosts_ExplicitVarSizeWithFlags_Flags[q7] ->
              sum([(sched_Explicit_Function1D[q6, q13] = hosts_ExplicitVarSizeWithFlags_Values[q7]) * crew_Function1D[q13] | q13 : int(1..n_boats)]) <=
              capacity_Function1D[hosts_ExplicitVarSizeWithFlags_Values[q7]]
                  | q7 : int(1..1 + (n_boats - 1))])
             | q6 : int(1..n_periods)]),
    and([sum([sched_Explicit_Function1D[q8, b1] = sched_Explicit_Function1D[q8, b2] | q8 : int(1..n_periods)]) <= 1
             | b1 : int(1..n_boats), b2 : int(1..n_boats), b1 != b2]),
    and([hosts_ExplicitVarSizeWithFlags_Flags[q1 + 1] -> hosts_ExplicitVarSizeWithFlags_Values[q1] < hosts_ExplicitVarSizeWithFlags_Values[q1 + 1]
             | q1 : int(1..1 + (n_boats - 1) - 1)]),
    and([hosts_ExplicitVarSizeWithFlags_Flags[q1] = false -> hosts_ExplicitVarSizeWithFlags_Values[q1] = 1 | q1 : int(1..1 + (n_boats - 1))]),
    and([hosts_ExplicitVarSizeWithFlags_Flags[q1 + 1] -> hosts_ExplicitVarSizeWithFlags_Flags[q1] | q1 : int(1..1 + (n_boats - 1) - 1)]),
    and([sched_Explicit_Function1D[q2, ..] <lex sched_Explicit_Function1D[q2 + 1, ..] | q2 : int(1..n_periods - 1)])

