language ESSENCE' 1.0

given n_periods: int(1..)
given maxl: int(1..)
given minl: int(1..)
given n_courses: int(1..)
given maxc: int(1..)
given minc: int(1..)
given fin1: int
given prereq_RelationAsMatrix: matrix indexed by [int(1..n_courses), int(1..n_courses)] of bool
given fin2: int
given fin3: int
given credits_Function1D: matrix indexed by [int(1..n_courses)] of int(1..fin3)
find curr_Function1D: matrix indexed by [int(1..n_courses)] of int(1..n_periods)
such that
    and([prereq_RelationAsMatrix[c1, c2] -> curr_Function1D[c1] < curr_Function1D[c2]
             | c1 : int(1..n_courses), c2 : int(1..n_courses)]),
    and([sum([toInt(curr_Function1D[q3] = p) * credits_Function1D[q3] | q3 : int(1..n_courses)]) <= maxl /\
         sum([toInt(curr_Function1D[q5] = p) * credits_Function1D[q5] | q5 : int(1..n_courses)]) >= minl
             | p : int(1..n_periods)]),
    and([sum([toInt(curr_Function1D[q8] = p) | q8 : int(1..n_courses)]) <= maxc /\
         sum([toInt(curr_Function1D[q11] = p) | q11 : int(1..n_courses)]) >= minc
             | p : int(1..n_periods)])

