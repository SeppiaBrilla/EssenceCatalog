    and([and([q4 > totalChildrenMatrix_ExplicitVarSizeWithMarker_Marker[q3] -> totalChildrenMatrix_ExplicitVarSizeWithMarker_Values[q3, q4] = 1
                  | q4 : int(1..1 + (numNodes - 1))])
             | q3 : int(1..numNodes)])

