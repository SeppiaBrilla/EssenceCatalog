                           | q12 : int(1..1 + (numNodes - 1))])
                       | q11 : int(1..1 + (numNodes - 1))])
              /\ linkCosts[node, directChildrenMatrix_ExplicitVarSizeWithMarker_Values[node, q6]] != 0
              /\ nodeBroadcastPower[node] >= linkCosts[node, directChildrenMatrix_ExplicitVarSizeWithMarker_Values[node, q6]]
                  | q6 : int(1..1 + (numNodes - 1))])
             | node : int(1..numNodes)]),
    and([and([q2 + 1 <= directChildrenMatrix_ExplicitVarSizeWithMarker_Marker[q1] ->
              directChildrenMatrix_ExplicitVarSizeWithMarker_Values[q1, q2] < directChildrenMatrix_ExplicitVarSizeWithMarker_Values[q1, q2 + 1]
                  | q2 : int(1..1 + (numNodes - 1) - 1)])
             | q1 : int(1..numNodes)]),
    and([and([q2 > directChildrenMatrix_ExplicitVarSizeWithMarker_Marker[q1] -> directChildrenMatrix_ExplicitVarSizeWithMarker_Values[q1, q2] = 1
                  | q2 : int(1..1 + (numNodes - 1))])
             | q1 : int(1..numNodes)]),
    and([and([q4 + 1 <= totalChildrenMatrix_ExplicitVarSizeWithMarker_Marker[q3] ->
