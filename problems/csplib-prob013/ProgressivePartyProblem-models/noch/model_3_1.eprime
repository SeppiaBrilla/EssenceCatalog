    and([and([aux1_Occurrence[q8, q11] -> or([sched_ExplicitR9_Function1D[q8, q15] = q11 | q15 : int(1..n_boats)])
                  | q11 : int(1..n_boats)])
             | q8 : int(1..n_periods)]),
    and([and([aux1_Occurrence[q8, q10] ->
              or([q17 <= hosts_ExplicitVarSizeWithMarker_Marker /\ hosts_ExplicitVarSizeWithMarker_Values[q17] = q10
                      | q17 : int(1..n_boats)])
                  | q10 : int(1..n_boats)])
             | q8 : int(1..n_periods)]),
    and([and([q21 <= hosts_ExplicitVarSizeWithMarker_Marker ->
              sched_ExplicitR9_Function1D[q20, hosts_ExplicitVarSizeWithMarker_Values[q21]] =
              hosts_ExplicitVarSizeWithMarker_Values[q21]
                  | q21 : int(1..n_boats)])
             | q20 : int(1..n_periods)]),
    and([and([q23 <= hosts_ExplicitVarSizeWithMarker_Marker ->
              sum([toInt(sched_ExplicitR9_Function1D[q22, q25] = hosts_ExplicitVarSizeWithMarker_Values[q23]) *
                   crew_Function1D[q25]
                       | q25 : int(1..n_boats)])
              <= capacity_Function1D[hosts_ExplicitVarSizeWithMarker_Values[q23]]
                  | q23 : int(1..n_boats)])
             | q22 : int(1..n_periods)]),
    and([b1 < b2 ->
         sum([toInt(sched_ExplicitR9_Function1D[q7, b1] = sched_ExplicitR9_Function1D[q7, b2])
                  | q7 : int(1..n_periods)])
         <= 1
             | b1 : int(1..n_boats), b2 : int(1..n_boats)]),
    and([q1 + 1 <= hosts_ExplicitVarSizeWithMarker_Marker ->
         hosts_ExplicitVarSizeWithMarker_Values[q1] < hosts_ExplicitVarSizeWithMarker_Values[q1 + 1]
             | q1 : int(1..n_boats - 1)]),
    and([q2 > hosts_ExplicitVarSizeWithMarker_Marker -> hosts_ExplicitVarSizeWithMarker_Values[q2] = 1
             | q2 : int(1..n_boats)]),
    and([sched_ExplicitR9_Function1D[q4, ..] <lex sched_ExplicitR9_Function1D[q4 + 1, ..] | q4 : int(1..n_periods - 1)])

