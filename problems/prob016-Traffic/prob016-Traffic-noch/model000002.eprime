    and([valid_ExplicitVarSizeWithFlags_Flags[q1 + 1] ->
         valid_ExplicitVarSizeWithFlags_Values_Function1D_1[q1, ..] <lex valid_ExplicitVarSizeWithFlags_Values_Function1D_1[q1 + 1, ..] \/
