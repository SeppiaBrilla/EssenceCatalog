                       or([valid_ExplicitVarSizeWithFlags_Values_Function1D_1[q2, rd] = 3 /\ valid_ExplicitVarSizeWithFlags_Values_Function1D_2[q2, rd] = 2,
                           valid_ExplicitVarSizeWithFlags_Values_Function1D_1[q2, rd % n_roads + 1] = 3 /\
                           valid_ExplicitVarSizeWithFlags_Values_Function1D_2[q2, rd % n_roads + 1] = 2; int(1..2)]); int(1..2)])
                  /\
