             | q1 : int(1..factorial((1 + (4 - 1)) * (1 + (2 - 1))) / (factorial(1 + (n_roads - 1)) * factorial((1 + (4 - 1)) * (1 + (2 - 1)) - (1 + (n_roads - 1)))))]),
    and([valid_ExplicitVarSizeWithFlags_Flags[q1 + 1] -> valid_ExplicitVarSizeWithFlags_Flags[q1]
