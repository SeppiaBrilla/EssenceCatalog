                  | q17 : int(1..capacity)])
             | q15 : int(1..nrings * nrings)])
such that
    and([or([q19 <= network_ExplicitWithRepetitionR4_Flag /\
             and([or([network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Flags[q19, q22] /\
                      network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Values[q19, q22] = q20
                          | q22 : int(1..capacity)])
                      | q20 : int(1..nnodes), demand_ExplicitR2_Occurrence[q18, q20]])
                 | q19 : int(1..nrings * nrings)])
             | q18 : int(1..fin1)]),
    and([q1 + 1 <= network_ExplicitWithRepetitionR4_Flag ->
         network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Flags[q1, ..] <lex
         network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Flags[q1 + 1, ..]
         \/
         and([network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Flags[q1, q11] =
              network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Flags[q1 + 1, q11]
                  | q11 : int(1..capacity)])
         /\ and([or([q11 = q12 | q12 : int(1..capacity)]) | q11 : int(1..capacity)])
         /\ and([or([q11 = q12 | q12 : int(1..capacity)]) | q11 : int(1..capacity)])
         /\
         network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Values[q1, ..] <=lex
         network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Values[q1 + 1, ..]
             | q1 : int(1..nrings * nrings - 1)]),
    and([q2 > network_ExplicitWithRepetitionR4_Flag ->
         and([network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Flags[q2, q13] = false
                  | q13 : int(1..capacity)])
         /\
         and([network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Values[q2, q14] = 1
                  | q14 : int(1..capacity)])
             | q2 : int(1..nrings * nrings)]),
    nrings = network_ExplicitWithRepetitionR4_Flag,
    and([q5 <= network_ExplicitWithRepetitionR4_Flag ->
         and([network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Flags[q5, q6 + 1] ->
              network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Values[q5, q6] <
              network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Values[q5, q6 + 1]
                  | q6 : int(1..capacity - 1)])
             | q5 : int(1..nrings * nrings)]),
    and([q5 <= network_ExplicitWithRepetitionR4_Flag ->
         and([network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Flags[q5, q7] = false ->
              network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Values[q5, q7] = 1
                  | q7 : int(1..capacity)])
             | q5 : int(1..nrings * nrings)]),
    and([q5 <= network_ExplicitWithRepetitionR4_Flag ->
         and([network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Flags[q5, q8 + 1] ->
              network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Flags[q5, q8]
                  | q8 : int(1..capacity - 1)])
             | q5 : int(1..nrings * nrings)]),
    and([q5 <= network_ExplicitWithRepetitionR4_Flag ->
         sum([toInt(network_ExplicitWithRepetitionR4_Values_ExplicitVarSizeWithFlags_Flags[q5, q9])
                  | q9 : int(1..capacity)])
         <= capacity
             | q5 : int(1..nrings * nrings)])

