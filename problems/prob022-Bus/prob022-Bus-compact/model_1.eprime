                  -> shifts_Explicit_Occurrence[q17, q19]
                      | q19 : int(1..Tasks_EnumSize)])
                 | q17 : int(1..fin1)])
             | q10 : int(1..2 ** (1 + (Tasks_EnumSize - 1)))]),
    and([1 >=
         sum([q11 <= sched_PartitionAsSet_ExplicitVarSizeWithMarker_Marker /\
              sched_PartitionAsSet_ExplicitVarSizeWithMarker_Values_Occurrence[q11, q1]
                  | q11 : int(1..2 ** (1 + (Tasks_EnumSize - 1)))])
             | q1 : int(1..Tasks_EnumSize)]),
    and([q12 <= sched_PartitionAsSet_ExplicitVarSizeWithMarker_Marker ->
         sum([sched_PartitionAsSet_ExplicitVarSizeWithMarker_Values_Occurrence[q12, q20]
                  | q20 : int(1..Tasks_EnumSize)])
         >= 1
             | q12 : int(1..2 ** (1 + (Tasks_EnumSize - 1)))]),
    and([q5 + 1 <= sched_PartitionAsSet_ExplicitVarSizeWithMarker_Marker ->
         sched_PartitionAsSet_ExplicitVarSizeWithMarker_Values_Occurrence[q5, ..] <lex
         sched_PartitionAsSet_ExplicitVarSizeWithMarker_Values_Occurrence[q5 + 1, ..]
             | q5 : int(1..2 ** (1 + (Tasks_EnumSize - 1)) - 1)]),
    and([q6 > sched_PartitionAsSet_ExplicitVarSizeWithMarker_Marker ->
         and([sched_PartitionAsSet_ExplicitVarSizeWithMarker_Values_Occurrence[q6, q21] =
              false
                  | q21 : int(1..Tasks_EnumSize)])
             | q6 : int(1..2 ** (1 + (Tasks_EnumSize - 1)))])

