             | q15 : int(1..2 ** (1 + (Tasks_EnumSize - 1)))])
such that
    and([q10 <= sched_PartitionAsSet_ExplicitVarSizeWithMarker_Marker ->
         or([and([sched_PartitionAsSet_ExplicitVarSizeWithMarker_Values_Occurrence[q10,
